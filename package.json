{
  "name": "mdbuttonwithspinner",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack && webpack-dev-server",
    "compile:postcss": "postcss  --use precss --use postcss-cssnext -d lib src/*.css",
    "compile:source": "babel src --out-dir lib",
    "clean": "rimraf lib dist",
    "prepublish": "npm-run-all clean --parallel compile:source compile:postcss",
    "test": "mocha --require babel-register --require ignore-styles src/__tests__/**.spec.js"
  },
  "keywords": [],
  "author": "zhenghe",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "npm run prepublish"
    }
  },
  "dependencies": {
    "mdbutton": "0.0.4",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-spinkit": "1.1.7"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "css-loader": "0.23.1",
    "expect": "1.20.1",
    "expect-jsx": "2.6.0",
    "ghooks": "1.2.4",
    "ignore-styles": "4.0.0",
    "mocha": "2.5.3",
    "npm-run-all": "2.2.2",
    "postcss": "5.0.21",
    "postcss-cli": "2.5.2",
    "postcss-cssnext": "2.6.0",
    "postcss-loader": "0.9.1",
    "precss": "1.4.0",
    "react-addons-test-utils": "15.1.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
